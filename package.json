{
  "name": "ramme",
  "version": "4.0.0",
  "author": "Anatoli Nicolae <anatoli@ramme.co>",
  "contributors": [
    {
      "author": "Terkel Gjervig",
      "email": "terkel@ramme.co"
    },
    {
      "author": "Bil Elmoussaoui",
      "email": "bil@ramme.co"
    },
    {
      "author": "Elijah Duffy",
      "email": "elijah@ramme.co"
    }
  ],
  "description": "Unofficial Instagram Desktop App",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "yarn run pack:main && yarn run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "yarn run lint:fix"
  },
  "build": {
    "productName": "ramme",
    "appId": "com.terkel.ramme",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns",
      "category": "public.app-category.social-networking"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons",
      "synopsis": "Unofficial Instagram Desktop App",
      "category": "Development",
      "target": [
        "deb",
        "rpm",
        "pacman",
        "tar.gz"
      ]
    }
  },
  "dependencies": {
    "axios": "^0.17.1",
    "electron-positioner": "^3.0.0",
    "electron-settings": "^3.1.4",
    "electron-window-state": "^4.1.1",
    "element-ui": "^2.2.0",
    "hasha": "^3.0.0",
    "instagram-private-api": "^0.6.7",
    "lodash": "^4.17.5",
    "mkdirp": "^0.5.1",
    "normalize.css": "^8.0.0",
    "touch": "^3.1.0",
    "vue": "^2.5.13",
    "vue-agile": "^0.3.7",
    "vue-click-helper": "^0.0.2",
    "vue-electron": "^1.0.6",
    "vue-perfect-scrollbar": "^0.1.0",
    "vue-router": "^3.0.1",
    "vue-timeago": "^3.4.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chalk": "^2.3.1",
    "copy-webpack-plugin": "^4.4.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^1.8.2",
    "electron-builder": "^20.0.4",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.3",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.0",
    "resolve-url-loader": "^2.2.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "url-loader": "^0.6.2",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^14.1.1",
    "vue-style-loader": "^4.0.2",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-hot-middleware": "^2.21.0"
  }
}
